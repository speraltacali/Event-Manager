<body>
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-85 p-r-85 p-t-55 p-b-55">
                <form class="login100-form validate-form flex-sb flex-w" asp-action="NuevoEvento" asp-controller="TipoEvento" method="post">
                    <span class="login100-form-title p-b-32">
                        Agregue una Descricpion
                    </span>

                    <span class="txt1 p-b-1">
                        Descripcion Del Tipo De Evento
                    </span>
                    <div class="wrap-input100 validate-input m-b-36" data-validate="Descripcion Requerido">
                        <input asp-for="Descripcion" class="input100" type="text" name="Descripcion">
                        <span class="focus-input100"></span>
                    </div>


                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Crear
                        </button>
                    </div>
                    
                </form>

            </div>
        </div>
    </div>

    <div id="dropDownSelect1"></div>

</body>

