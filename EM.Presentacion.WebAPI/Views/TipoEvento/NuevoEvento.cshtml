<div class="limiter">
    <div class="container-login100">
        <div class="wrap-login100 p-l-85 p-r-85 p-t-55 p-b-55">
            <form class="login100-form validate-form flex-sb flex-w" asp-action="Create" asp-controller="Usuario" method="post">
                <span class="login100-form-title p-b-32">
                    Agregue una Descricpion
                </span>

                <span class="txt1 p-b-11">
                    Descripcion Del Tipo De Evento
                </span>
                <div class="wrap-input100 validate-input m-b-36" data-validate="Apellido Requerido">
                    <input asp-for="TipoEventoDescripcion" class="input100" type="text" name="TipoEventoDescripcion">
                    <span class="focus-input100"></span>
                </div>

                <div class="flex-sb-m w-full p-b-48"></div>

                <div class="container-login100-form-btn">
                    <button class="login100-form-btn" style="width:500px;height:80px;">
                        Crear
                    </button>
                </div>

            </form>

        </div>
    </div>
</div>

